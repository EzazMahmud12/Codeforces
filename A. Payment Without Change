#include<bits/stdc++.h>
using namespace std;
#define e4 ios_base::sync_with_stdio(false); cin.tie(NULL)
#define endl '\n'
#define ll  long long
#define test_case int tc;cin>>tc;while(tc--)


int main() {


		 e4;

		 test_case{
		 ll a,b,n,s;
 
		 cin>>a>>b>>n>>s;
 
		 ll val,mn;

		 mn=min(s/n,a);
 
		 val=s-(mn*n);	 
 
		 if(b>=val) cout<<"YES"<<endl;
		 else cout<<"NO"<<endl;

		}

}

			
		    //            S m o t h e r e d  M a t e !!		 //	
