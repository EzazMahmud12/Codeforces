#include<bits/stdc++.h>
using namespace std;
#define e4 ios_base::sync_with_stdio(false); cin.tie(NULL)
#define endl '\n'
#define ll  long long
#define test_case int tc;cin>>tc;while(tc--)


int main() {


		e4;

		test_case{

		int i;

		string s;

		cin>>s;

		for(int j=1; j<=100; j++){

		for(i=1; i<s.size(); i++){
			int a,b;
			a=s[i]-'0';
			b=s[i-1]-'0';
			a--;
			if(a>b){
				s[i]--;
				swap(s[i],s[i-1]);
			}
		}
		
	}

		cout<<s<<endl;

	}


		
}

		



	     //            S m o t h e r e d  M a t e !!		 //
