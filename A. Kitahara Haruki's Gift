#include<bits/stdc++.h>
using namespace std;
#define e4 ios_base::sync_with_stdio(false); cin.tie(NULL)
#define endl '\n'
#define ll  long long
#define test_case int tc;cin>>tc;while(tc--)


int main() {


		e4;

		int i,n;

		cin>>n;

		int c1=0,c2=0;

		for(i=0; i<n; i++){
			int a;
			cin>>a;
			if(a==100) c1++;
			else c2++;
		}

		if(c1>0){
			if(c1%2) cout<<"NO";
			else cout<<"YES";
		}
		else{
			if(c2%2) cout<<"NO";
			else cout<<"YES";
		}
	
}

	
