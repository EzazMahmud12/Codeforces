#include<bits/stdc++.h>
using namespace std;
#define e4 ios_base::sync_with_stdio(false); cin.tie(NULL)
#define endl '\n'
#define ll  long long
#define test_case int tc;cin>>tc;while(tc--)


int main() {


		e4;

		 int i;

         string s,ans;

         cin>>s;

         for(i=0; i<s.size(); i++){
            if(s[i]==ans.back()){
               ans.pop_back();
            }
            else{
               ans.push_back(s[i]);
            }
         }

         cout<<ans;

			
}

	
