#include<bits/stdc++.h>
using namespace std;
#define e4 ios_base::sync_with_stdio(false); cin.tie(NULL)
#define endl '\n'
#define ll  long long
#define test_case int tc;cin>>tc;while(tc--)


int main() {


		e4;

		test_case{
		
		ll c=0,n,t=10000;

		bool ok=true;

		cin>>n;

		while(t--){

			if(n==1){
				break;
			}

			if(n%6==0){
				n=n/6;
			}
			else{
				n=n*2;
				if(n<6){
					ok=false;
					break;
				}
			}

			c++;
		}

		if(ok && n==1)cout<<c<<endl;
		else cout<<-1<<endl;
 
}

			
}

	
		    //            S m o t h e r e d  M a t e !!		 //	
