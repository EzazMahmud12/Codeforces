#include<bits/stdc++.h>
using namespace std;
#define e4 ios_base::sync_with_stdio(false); cin.tie(NULL)
#define endl '\n'
#define ll  long long
#define test_case int tc;cin>>tc;while(tc--)


int main() {


			e4;

			ll i,n;

			cin>>n;

			ll sum=0;

			for(i=0; i<n; i++){
				ll a;
				cin>>a;

				sum+=a;
			}

			if(sum%n==0) cout<<n;
			else cout<<n-1;
}

	
